using System;

namespace _16
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите N");
            int n = Convert.ToInt32(Console.ReadLine());
            if (n > 0)
            {
                int[] a = new int[n];
                for (int i = 0; i < a.Length; i++)
                {
                    a[i] = i * 2 + 1;
                }
                for (int i = 0; i < a.Length; i++)
                {
                    Console.Write(" " + a[i]);
                }
            }
            else
            {
                Console.WriteLine("Ошибка ввода");
            }
        }
    }
}
